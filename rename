<?php
require 'PhotoUpload.php';


function rename_photo($old_name, $new_name) {
    $updated_info =  \Cloudinary\Uploader::rename($old_name, $new_name);
    var_dump($updated_info);
}

$photos = R::find('photo', 'public_id LIKE ?', array('%.j%'));

//var_dump(count($photos));

foreach ($photos as $photo) {
    $trimName = preg_replace('/\\.[^.\\s]{3,4}$/', '',$photo->public_id);
    //var_dump($photo->public_id);
    rename_photo($photo->public_id, $trimName);
}


